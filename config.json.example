 "base_url": "https://ashina-motors.com/shop/",
  "max_threads": 5,
  "timeout": 10,
  "headless": true,
  "page_load_delay": 2,
  "output_dir": "output",
  "images_dir": "output/images",
  "download_images": true,
  "fields": [
    "title",
    "price",
    "description",
    "sku",
    "oem",
    "images",
    "categories",
    "url"
  ],
  "selectors": {
    "product_links": "li.product a.woocommerce-LoopProduct-link",
    "title": "h2.woocommerce-loop-product__title",
    "price": "span.price .woocommerce-Price-amount",
    "description": ".woocommerce-product-details__short-description",
    "sku": ".product-meta",
    "oem": "p.oem",
    "images": ".woocommerce-product-gallery__image img, .woocommerce-loop-product__thumbnail-wrapper img",
    "categories": ".posted_in a",
    "pagination_next": "a.next.page-numbers"
  },
  "database": {
    "enabled": false,
    "host": "localhost",
    "port": 3306,
    "user": "root",
    "password": "",
    "database": "scraped_products",
    "table_prefix": "wp_"
  }
